<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gameon.mycash_carteiradigital">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SobreActivity"
            android:parentActivityName=".activity.HomeActivity"></activity>
        <activity android:name=".activity.GraficoActivity"
            android:parentActivityName=".activity.HomeActivity"/>
        <activity
            android:name=".activity.ListagemGanhosActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity
            android:name=".activity.HomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CadastroDespesasActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity
            android:name=".activity.CadastroGanhosActivity"
            android:parentActivityName=".activity.HomeActivity" />
    </application>

</manifest>